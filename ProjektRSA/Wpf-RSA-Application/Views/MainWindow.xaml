        <StackPanel Grid.Row="0" Grid.ColumnSpan="2"></StackPanel>
        <StackPanel Grid.Row="1" Grid.Column="0" Margin="15">
            
        </StackPanel>
        <StackPanel Grid.Row="1" Grid.Column="1" Margin="15">
            
        </StackPanel>
        <StackPanel Grid.Row="2" Grid.ColumnSpan="2" Margin="15">
            
        </StackPanel>
    </Grid>
